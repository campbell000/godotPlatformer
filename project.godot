config_version=5

[application]

config/name="Platformer Godot"
run/main_scene="res://levels/DemoLevel.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://icon.png"

[autoload]

Globals="*res://objects/Globals.gd"
Physics="*res://objects/Physics.gd"
Common="*res://characters/player/States/Common.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[display]

window/size/viewport_width=400
window/size/viewport_height=240
window/size/window_width_override=1200
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=2

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[global]

window=false
Snap=false
GPU=false
debug=false

